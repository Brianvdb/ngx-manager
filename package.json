{
    "name": "@bravobit/ngx-manager",
    "version": "0.0.60",
    "description": "The NGX manager library",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "license": "MIT",
    "keywords": [
        "angular",
        "angular4",
        "aot",
        "aot-compatible",
        "aot-compilation",
        "library",
        "ngx-manager",
        "typescript"
    ],
    "author": {
        "name": "Stan van Heumen",
        "url": "https://www.linkedin.com/in/stan-van-heumen-86b4ab10a"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/stanvanheumen/ngx-manager.git"
    },
    "bugs": {
        "url": "https://github.com/stanvanheumen/ngx-manager/issues"
    },
    "homepage": "https://github.com/stanvanheumen/ngx-manager#readme",
    "scripts": {
        "build": "npm run build:esm && npm run build:umd && gulp copyStyles && rimraf tmp",
        "build:esm": "gulp inline-templates && npm run ngcompile",
        "build:esm:watch": "gulp build:esm:watch",
        "build:umd": "webpack --config webpack-umd.config.ts",
        "build:umd:watch": "npm run build:umd -- --watch",
        "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
        "ci": "npm run build",
        "clean:all": "npm run clean:tmp && rimraf node_modules",
        "clean:tmp": "rimraf coverage dist tmp docs",
        "explorer": "source-map-explorer ./dist/index.umd.js",
        "lint": "npm run tslint 'src/**/*.ts'",
        "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
        "prebuild": "rimraf dist tmp",
        "prebuild:watch": "rimraf dist tmp",
        "prepublishOnly": "npm run ci",
        "preversion": "npm run ci",
        "tslint": "tslint"
    },
    "dependencies": {},
    "peerDependencies": {
        "@angular/animations": "^4.0.0",
        "@angular/common": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/router": "^4.0.0",
        "ng-select": "^1.0.0-beta.6",
        "ngx-cookie": "^1.0.0"
    },
    "devDependencies": {
        "@angular/animations": "^4.0.0",
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/compiler-cli": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/router": "^4.0.0",
        "@types/node": "^8.0.32",
        "@types/webpack": "^3.0.13",
        "@types/webpack-env": "^1.13.0",
        "angular2-template-loader": "^0.6.2",
        "awesome-typescript-loader": "^3.1.3",
        "concurrently": "^3.4.0",
        "css-loader": "^0.28.1",
        "gulp": "^3.9.1",
        "gulp-inline-ng2-template": "^4.0.0",
        "json-loader": "^0.5.4",
        "ng-select": "^1.0.0-beta.6",
        "ngx-cookie": "^1.0.0",
        "node-sass": "^4.5.2",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.6.2",
        "rxjs": "^5.3.1",
        "sass-loader": "^6.0.5",
        "source-map-explorer": "^1.3.3",
        "to-string-loader": "^1.1.5",
        "ts-node": "^3.0.4",
        "tslint": "^5.2.0",
        "typescript": "2.3.4",
        "webpack": "^3.6.0",
        "webpack-angular-externals": "^1.0.2",
        "webpack-rxjs-externals": "^1.0.0",
        "zone.js": "^0.8.10"
    }
}
